{
  "manifestVersion": 1,
  "id": "OutsystemsExt",
  "version": "0.1.2",
  "name": "OutSystems Integration",
  "scopes": [],
  "description": "OutSystems Platform Integration",
  "publisher": "joalmeid",
  "galleryFlags": [
    "Preview"
  ],
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "Outsystems"
  ],
  "categories": [
    "Integrate"
  ],
  "screenshots": [
    {
      "path": "static/images/screen1.png"
    }
  ],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://aka.ms/outsystems"
    },
    "support": {
      "uri": "https://aka.ms/outsystems"
    },
    "issues": {
      "uri": "https://aka.ms/outsystems"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://aka.ms/outsystems"
  },
  "branding": {
    "color": "rgb(204, 34, 0)",
    "theme": "dark"
  },
  "files": [
    {
      "path": "outsystemsreleasetask"
    }
  ],
  "contributions": [
    {
      "id": "outsystemsreleasetask",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "outsystemsreleasetask"
      }
    },
    {
      "id": "outsystems-service-endpoint",
      "description": "Service Endpoint type for OutSystems Platform connections",
      "type": "ms.vss-endpoint.service-endpoint-type",
      "targets": [
        "ms.vss-endpoint.endpoint-types"
      ],
      "properties": {
        "name": "Outsystems",
        "displayName": "Outsystems Platform",
        "url": {
          "displayName": "Server Url",
          "helpText": "Url for the Outsystems Lifetime to connect to."
        },
        "inputDescriptors": [
          {
            "id": "acceptUntrustedCerts",
            "name": "i18n:Accept untrusted SSL certificates",
            "description": "i18n:Allows the Outsystems Platform clients to accept self-signed SSL server certificates.",
            "inputMode": "checkbox",
            "isConfidential": false,
            "validation": {
              "isRequired": false,
              "dataType": "boolean"
            }
          }
        ],
        "dataSources": [
          {
            "name": "TestConnection",
            "endpointUrl": "{{endpoint.url}}/lifetimeapi/rest/v1/swagger.json",
            "resultSelector": "jsonpath:$.basePath"
          },
          {
            "name": "osLifetimeApps",
            "endpointUrl": "{{endpoint.url}}/lifetimeapi/rest/v1/applications"
          },
          {
            "name": "osLifetimeEnvironments",
            "endpointUrl": "{{endpoint.url}}/lifetimeapi/rest/v1/environments"
          },
          {
            "name": "osLifetimeDeployments",
            "endpointUrl": "{{endpoint.url}}/lifetimeapi/rest/v1/deployments"
          },
          {
            "name": "osLifetimeModules",
            "endpointUrl": "{{endpoint.url}}/lifetimeapi/rest/v1/modules"
          }
        ],
        "authenticationSchemes": [
          {
            "type": "ms.vss-endpoint.endpoint-auth-scheme-token"
          }
        ],
        "helpMarkDown": "<a href=\"https://aka.ms/outsystems\" target=\"_blank\"><b>Learn More</b></a>"
      }
    },
    {
      "id": "OutsystemsServiceHook",
      "type": "ms.vss-servicehooks.consumer",
      "targets": [
        "ms.vss-servicehooks.consumers"
      ],
      "properties": {
        "id": "OutsystemsConsumer",
        "name": "Outsystems",
        "description": "OutSystems low-code development platform has everything you need to build, deploy, manage and change enterprise mobile and web apps faster than ever. It provides you traceability and integration. <a href=\"https://aka.ms/outsystems\" target=\"_blank\"><b>Learn More</b></a> about how Outsytems integration with Visual Studio Team Services.",
        "informationUrl": "https://aka.ms/outsystems",
        "inputDescriptors": [
          {
            "id": "url",
            "isRequired": true,
            "name": "URL",
            "description": "URL to post event payload to",
            "inputMode": "textbox"
          }
        ],
        "actions": [
          {
            "id": "performAction",
            "name": "Perform action",
            "description": "Posts a standard event payload",
            "supportedEventTypes": [
              "git.push",
              "git.pullrequest.created",
              "git.pullrequest.updated"
            ],
            "publishEvent": {
              "url": "{{{url}}}",
              "resourceDetailsToSend": "all",
              "messagesToSend": "all",
              "detailedMessagesToSend": "all"
            }
          }
        ]
      }
    }
  ]
}